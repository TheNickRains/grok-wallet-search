# x.ai API Key (required)
xai_key=your_xai_api_key_here
# Alternative: XAI_API_KEY=your_xai_api_key_here

# Google Sheets Configuration (required)
GOOGLE_CREDENTIALS_FILE=path/to/your/google-credentials.json
GOOGLE_SHEET_ID=your_google_sheet_id_here

# GROK Model Configuration (optional)
GROK_MODEL=grok-4-fast

# Processing Configuration (optional)
WALLET_LIMIT=0

# Multi-Worksheet Support (optional)
WORKSHEET_NAME=Gigabud Holders
WORKSHEETS_TO_PROCESS=Gigabud Holders,Grass Claims

# Parallel Processing (optional)
USE_PARALLEL=true
MAX_CONCURRENT_REQUESTS=5

# Rate Limiting (optional)
RATE_LIMIT_DELAY=1
RATE_LIMIT_ERROR_DELAY=60
RATE_LIMIT_WINDOW=60
MAX_REQUESTS_PER_WINDOW=50

# Railway Configuration (optional)
RAILWAY_VOLUME_MOUNT_PATH=/tmp
